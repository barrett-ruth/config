#!/bin/sh

proj="$PWD"

case "$proj" in
*theCourseForum2*)
  # docker compose up --build
  cmd='docker compose up'
  ;;
*atlas*)
  cmd='pnpm run dev'
  ;;
*interview-prep*)
  cmd='pnpm run dev'
  ;;
*neovim*)
  cmd='make'
  ;;
*cses*|*usaco*|*codeforces*)
  cmd='python solution.py < input.txt'
  ;;
esac

echo " > $cmd" | sed "s|$HOME|~|g"
eval "$cmd"

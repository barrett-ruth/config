#!/bin/sh

proj="$PWD"

case "$(basename "$proj")" in
theCourseForum2)
    # docker compose up --build
    cmd='docker compose up'
    ;;
gitmap)
    cmd='python -m flask run --debug';
    ;;
certd)
    cmd='python main.py'
    ;;
*)
    echo "Project '$proj' unknown."
    exit
    ;;
esac

echo " > $cmd" | sed "s|$HOME|~|g"
eval "$cmd"

#!/usr/bin/env zsh

case "$1" in
    -) cd - >/dev/null && return;;
    '') cd ~;;
    certd) cd ~/Desktop/certd;;
    cp) cd ~/Desktop/competitive-programming;;
    dl) cd ~/Downloads;;
    site) cd ~/Desktop/barrett-ruth.github.io;;
    scr) cd "$SCRIPTS";;
    desk) cd ~/Desktop;;
    dots) cd ~/Desktop/dots;;
    tcf) cd ~/Desktop/theCourseForum2;;
    tm) cd ~/.config/tmux;;
    nv) cd ~/.config/nvim;;
    zsh) cd ~/.config/zsh;;
esac

#!/usr/bin/env zsh

case "$1" in
    -) cd - >/dev/null && return;;
    '') cd ~;;
    ip) cd ~/dev/swe-interview-prep;;
    dl) cd ~/dl;;
    sl) cd ~/dev/sl;;
    lz) cd ~/.local/share/nvim/lazy;;
    dev) cd ~/dev;;
    ds) cd ~/dev/data-structures;;
    site) cd ~/dev/barrettruth.com;;
    cf) cd ~/.config;;
    notes) cd ~/dev/notes;;
    scr) cd "$SCRIPTS";;
    desk) cd ~/Desktop;;
    dots) cd ~/dev/dots;;
    tcf) cd ~/dev/theCourseForum2;;
    trb) cd ~/dev/trb-capital-management;;
    tm) cd ~/.config/tmux;;
    nv) cd ~/.config/nvim;;
    x) cd ~/.config/X11;;
    zsh) cd ~/.config/zsh;;
    *) cd "$@";;
esac

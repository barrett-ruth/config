#!/usr/bin/env zsh

case "$1" in
    -) cd - >/dev/null && return;;
    '') cd ~;;
    certd) cd ~/dev/certd-backend;;
    cp) cd ~/dev/competitive-programming;;
    dl) cd ~/Downloads;;
    site) cd ~/dev/barrett-ruth.github.io;;
    scr) cd "$SCRIPTS";;
    dev) cd ~/dev;;
    dots) cd ~/dev/dots;;
    tcf) cd ~/dev/theCourseForum2;;
    tm) cd ~/.config/tmux;;
    nv) cd ~/.config/nvim;;
    zsh) cd ~/.config/zsh;;
esac

#!/usr/bin/env zsh

case "$1" in
    -) cd - >/dev/null && return;;
    '') cd ~;;
    dl) cd ~/dl;;
    scr) cd "$SCRIPTS";;
    dev) cd ~/dev;;
    dmenu) cd ~/dev/sl/dmenu;;
    dots) cd ~/dev/dots;;
    dwm) cd ~/dev/sl/dwm;;
    dwmb) cd ~/dev/sl/dwmb;;
    st) cd ~/dev/sl/st;;
    tcf) cd ~/dev/theCourseForum2;;
    nv) cd ~/.config/nvim;;
    zsh) cd ~/.config/zsh;;
esac

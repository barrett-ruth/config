#!/usr/bin/env zsh

case "$1" in
    -) cd - >/dev/null && return;;
    '') cd ~;;
    atlas) cd ~/Desktop/atlas;;
    ip) cd ~/Desktop/interview-prep;;
    dl) cd ~/Downloads;;
    lz) cd ~/.local/share/nvim/lazy;;
    site) cd ~/Desktop/barrett-ruth.github.io;;
    notes) cd ~/Desktop/notes;;
    scr) cd "$SCRIPTS";;
    desk) cd ~/Desktop;;
    dots) cd ~/Desktop/dots;;
    tcf) cd ~/Desktop/theCourseForum2;;
    tm) cd ~/.config/tmux;;
    nv) cd ~/.config/nvim;;
    zsh) cd ~/.config/zsh;;
esac
